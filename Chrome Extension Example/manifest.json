{
	"manifest_version": 2,

	"name": "DaTa ScApInG iS sO cOoL",
	"description": "The coolest chrome extension ever",
	"version": "1.0",

	"background": {
		"scripts": ["popup.js"],
		"persistent": true
	},

	"permissions": [
		"tabs",
		"<all_urls>"
    ],
	"browser_action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	}
}