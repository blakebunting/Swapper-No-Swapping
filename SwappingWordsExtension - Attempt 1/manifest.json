{
  "name": "Big Donny",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Changes Donald Trumps name",
  "author": "Blake Bunting",

  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "penis.png"
  }
}
